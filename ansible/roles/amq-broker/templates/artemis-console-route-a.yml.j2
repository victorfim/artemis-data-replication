kind: Route
apiVersion: route.openshift.io/v1
metadata:
  name: artemis-a-console
  namespace: {{ dc_a_artemis_namespace }}
  labels:
    ActiveMQArtemis: {{ dc_a_artemis_namespace }}
    application: {{ dc_a_artemis_namespace }}-app
  annotations:
    openshift.io/host.generated: 'true'
spec:
  host: amq-console-{{ dc_a_artemis_namespace }}.{{ dc_domain }}
  to:
    kind: Service
    name: {{ dc_a_artemis_namespace }}-wconsj-0-svc
    weight: 100
  port:
    targetPort: wconsj
  tls:
    termination: edge
  wildcardPolicy: None

